{
  "name": "@takumi0706/google-calendar-mcp",
  "version": "0.1.0",
  "description": "Google Calendar MCP server for Claude Desktop integration",
  "main": "dist/index.js",
  "bin": {
    "google-calendar-mcp": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "postbuild": "chmod +x dist/index.js",
